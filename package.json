{
  "name": "garmin-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for Garmin running workout data integration with Claude Desktop",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "garmin-mcp-server": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc --watch & node --watch dist/index.js",
    "download": "tsx scripts/download-data.ts",
    "test:mcp": "scripts/test-mcp.sh",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "mcp",
    "garmin",
    "running",
    "workouts",
    "health",
    "fitness"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.1",
    "better-sqlite3": "^12.5.0",
    "garmin-connect": "^1.6.2",
    "node-fetch": "^3.3.2",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^25.0.3",
    "@vitest/ui": "^4.0.16",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "files": [
    "dist"
  ]
}
